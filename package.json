{
  "name": "pure-dialog",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm -C apps/web dev",
    "build": "pnpm -w -r build",
    "start": "node server.js",
    "preview": "pnpm -C apps/web preview",
    "setup": "bash setup.sh",
    "deploy": "bash deploy.sh",
    "deploy:appengine": "gcloud app deploy app.yaml --quiet",
    "typecheck": "pnpm -w -r run typecheck",
    "lint": "pnpm -w -r run lint",
    "lint:fix": "pnpm -w -r exec eslint . --ext .ts,.tsx,.js,.mjs --fix",
    "gcp-build": "npm run build"
  },
  "devDependencies": {
    "@effect/eslint-plugin": "^0.3.2",
    "@effect/language-service": "^0.27.2",
    "@effect/platform-node": "^0.91.0",
    "@effect/vitest": "^0.24.1",
    "@eslint/js": "^9.31.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.34.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^6.2.0"
  },
  "dependencies": {
    "@effect/experimental": "^0.55.0",
    "@effect/platform": "0.88.2",
    "@effect/platform-node": "^0.91.0",
    "@google/genai": "^1.20.0",
    "dotenv": "^16.4.5",
    "effect": "^3.16.16",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
